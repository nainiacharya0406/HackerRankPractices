public class Solution3 {
	public ListNode addTwoNumbers(ListNode l1, ListNode l2) {
        int sum1 = 0;
			ListNode l3 = new ListNode(0);
      ListNode curr = l3;
		while(l1.next != null) {
			sum1  = l1.val +l2.val;
			curr.next = new ListNode(sum1%10);
          curr = curr.next;
			l1 = l1.next; l2 = l2.next;
			return l3;
		}
		return null;
  }
}

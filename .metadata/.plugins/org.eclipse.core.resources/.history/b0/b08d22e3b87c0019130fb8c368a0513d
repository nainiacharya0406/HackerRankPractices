class Solution1 {
	public static void main(String args[]) {
		int nums[] = {2, 7, 11, 15};
		twoSum(nums, 9);
	}

	public static int[] twoSum(int[] nums, int target) {
		int ans[] = new int[];
		for(int i=0; i<nums.length -1; i++) {
			if(nums[i]+nums[i+1] == target) {
				ans[0] = i;
				ans[1] = i+1;
				return ans;
			}				
		}
		return null;
	}
}
package warmup;

public class PatterPrintRightAngledTriangle {

	static void staircase(int n) {
		for(int i=0; i<n ;i++) {
			for(int j=i; j<n; j++) {
				for(int k=0; k<n; k++) {
					System.out.println("#");
				}
			}
		}

    }
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		staircase(4);

	}

}

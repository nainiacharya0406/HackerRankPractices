public class Solution3 {
	public ListNode addTwoNumbers(ListNode l1, ListNode l2) {
        ListNode l3 = new ListNode();
        int sum1 = 0;
        int sum2 = 0;
        int sum3 = 0;
        int i=0;
		while(l1.next != null) {
			sum1 = (int) sum1 + (l1.val * Math.pow(10, i));
			l1 = l1.next;
			i++;
		}
		return l3;
    }
}
